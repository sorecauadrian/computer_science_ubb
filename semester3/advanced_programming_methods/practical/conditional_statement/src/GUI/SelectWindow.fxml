<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<BorderPane prefHeight="625.0" prefWidth="788.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SelectWindow_Controller">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="select_programs_list_view" />
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="182.0" prefWidth="788.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="select_button" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#select_program" prefHeight="70.0" prefWidth="368.0" text="Select Program" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
