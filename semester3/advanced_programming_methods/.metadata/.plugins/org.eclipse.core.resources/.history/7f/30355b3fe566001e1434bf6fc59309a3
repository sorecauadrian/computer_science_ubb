package Model;

public class Students implements Participant
{
	private String name;
	private boolean presented;
	
	public Students(String name, boolean presented)
	{
		this.name = name;
		this.presented = presented;
	}
	
	public String get_name() {return this.name;}
	public String get_role() {return "student";}
	public String to_string() {return this.get_role() + ": " + this.get_name() + "";}
	
	public boolean presented() {return this.presented;}
	
}
